{
    "name": "Volunteer",
    "base": "User",
    "idInjection": true,
    "options": {
        "validateUpsert": true,
        "mysql": {
            "table": "personas"
        }
    },
    "hidden": ["mesayuda"],
    "properties": {
        "name": {
            "type": "string",
            "required": true,
            "length": 60,
            "mysql": {
                "columnName": "nombre",
                "nullable": "N"
            }
        },
        "phone": {
            "type": "string",
            "required": true,
            "length": 30,
            "mysql": {
                "columnName": "celular",
                "nullable": "N"
            }
        },
        "email": {
            "type": "string",
            "required": true,
            "length": 100,
            "mysql": {
                "columnName": "email",
                "nullable": "N"
            }
        },
        "birthdate": {
            "type": "string",
            "default": "",
            "length": 10,
            "mysql": {
                "columnName": "fecha_nacimiento",
                "nullable": "N"
            }
        },
        "mesayuda": {
            "type": "string",
            "default": "",
            "length": 1,
            "mysql": {
                "columnName": "mesayuda",
                "nullable": "N"
            }
        },
        "district": {
            "type": "string",
            "default": "",
            "length": 150,
            "mysql": {
                "columnName": "localidad",
                "nullable": "N"
            }
        },
        "helpWith": {
            "type": "string",
            "default": "",
            "length": 200,
            "mysql": {
                "columnName": "colaborar",
                "nullable": "N"
            }
        },
        "profession": {
            "type": "string",
            "default": "",
            "length": 200,
            "mysql": {
                "columnName": "profesion",
                "nullable": "N"
            }
        },
        "comments": {
            "type": "string",
            "default": "",
            "length": 700,
            "mysql": {
                "columnName": "comentarios",
                "nullable": "N"
            }
        },
        "projectsInCharge": {
            "type": "string",
            "default": "no",
            "length": 10,
            "mysql": {
                "columnName": "proyectos",
                "nullable": "N"
            }
        },
        "collectThings": {
            "type": "string",
            "default": "no",
            "length": 10,
            "mysql": {
                "columnName": "juntar_cosas",
                "nullable": "N"
            }
        },
        "keepUpdated": {
            "type": "string",
            "default": "no",
            "length": 10,
            "mysql": {
                "columnName": "recibir_info",
                "nullable": "N"
            }
        },
        "createdAt": {
            "type": "date",
            "defaultFn": "now",
            "mysql": {
                "columnName": "fecha",
                "dataType": "DATE",
                "nullable": "N"
            }
        },
        "updatedAt": {
            "type": "date",
            "defaultFn": "now",
            "dataType": "timestamp",
            "mysql": {
                "columnName": "actualizado",
                "dataType": "timestamp",
                "nullable": "N"
            }
        },
        "created": false,
        "lastUpdated": false,
        "status": false,
        "emailVerified": false,
        "credentials": false,
        "password": false,
        "username": false,
        "challenges": false,
        "realm": false,
        "verificationToken": false
    },
    "validations": [],
    "relations": {
        "loginCode": {
            "type": "hasOne",
            "model": "LoginCode",
            "foreignKey": "volunteerId"
        },
        "projects": {
            "type": "hasMany",
            "model": "Project",
            "foreignKey": "volunteerId",
            "through": "Subscription"
        }
    },
    "acls": [
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "admin",
            "permission": "ALLOW"
        },
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$unauthenticated",
            "permission": "ALLOW",
            "property": "create"
        },
        {
            "accessType": "EXECUTE",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "ALLOW",
            "property": "login"
        },
        {
            "accessType": "EXECUTE",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "ALLOW",
            "property": "sendLoginCode"
        },
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$owner",
            "permission": "ALLOW",
            "property": "updateAttributes"
        },
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$owner",
            "permission": "ALLOW",
            "property": "destroyById"
        },
        {
            "accessType": "READ",
            "principalType": "ROLE",
            "principalId": "$owner",
            "permission": "ALLOW",
            "property": "__get__projects"
        },
        {
            "accessType": "READ",
            "principalType": "ROLE",
            "principalId": "$owner",
            "permission": "ALLOW",
            "property": "__exists__projects"
        },
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "__link__projects"
        },
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$owner",
            "permission": "ALLOW",
            "property": "__unlink__projects"
        }
    ],
    "methods": [],
    "mixins":{
        "DisableAllMethods":{
            "expose":[
                "login",
                "sendLoginCode",
                "logout",
                "deleteById",
                "create",
                "updateAttributes",
                "findById",
                "__get__projects",
                "__link__projects",
                "__unlink__projects",
                "__exists__projects"
            ]
        }
    }
}
