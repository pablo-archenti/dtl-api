{
    "name": "DeviceToken",
    "base": "PersistedModel",
    "idInjection": false,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "token": {
            "type": "string",
            "length": 512,
            "index": {"unique": true},
            "id": "true"
        },
        "createdAt": {
            "type": "date",
            "defaultFn": "now",
            "dataType": "timestamp"
        },
        "updatedAt": {
            "type": "date",
            "defaultFn": "now",
            "dataType": "timestamp"
        }
    },
    "validations": [],
    "relations": {},
    "acls": [
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY"
        },
        {
            "accessType": "*",
            "principalType": "ROLE",
            "principalId": "admin",
            "permission": "ALLOW"
        },
        {
            "accessType": "WRITE",
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "ALLOW",
            "property": "upsert"
        }
    ],
    "methods": [],
    "mixins":{
        "DisableAllMethods":{
            "expose":[
                "upsert"
            ]
        }
    }
}
